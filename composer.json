{
    "name": "pentangle/laravel-backpack-impersonation-addon",
    "description": "Backpack for Laravel Addon allows backpack users to impersonate other users",
    "keywords": [
        "Pentangle",
        "laravel-backpack-impersonation-addon"
    ],
    "homepage": "https://github.com/pentangle/laravel-backpack-impersonation-addon",
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ©an Poynter-Smith",
            "email": "spoyntersmith@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Pentangle\\LaravelBackpackImpersonationAddon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pentangle\\LaravelBackpackImpersonationAddon\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
